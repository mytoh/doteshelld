## -*- mode: eshell-script -*-

(when (require 'eshellar nil 'noerror)

  (add-to-list 'eshellar:cellars "~/.shellar_custom")
  (add-to-list 'eshellar:cellars "~/.config/shellar")

  (setq eshellar:bottles
        '(huone freebsd build colour
          completion option alias pikkukivi
          clip mesh did-you-mean z esh-buf-stack
          xterm-color
          prompt
          eshell-fringe-status))
  ;; eshell-git-prompt

  (eshellar:init))
