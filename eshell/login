## -*- mode: eshell-script -*-

(when (require 'eshellar nil 'noerror)

  (add-to-list 'eshellar:cellars "~/.shellar_custom")
  (add-to-list 'eshellar:cellars "~/.config/shellar")

  (setq eshellar:bottles
        '(huone prompt freebsd build colour completion option alias pikkukivi))

  (eshellar:init))
